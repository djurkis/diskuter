        function SmeClUserError(a,b,c){a=Error(a,b,c);a.user={state:"error"};Object.setPrototypeOf(a,Object.getPrototypeOf(this));Error.captureStackTrace&&Error.captureStackTrace(a,SmeClUserError);return a}SmeClUserError.prototype=Object.create(Error.prototype,{constructor:{value:Error,enumerable:!1,writable:!0,configurable:!0}});Object.setPrototypeOf?Object.setPrototypeOf(SmeClUserError,Error):SmeClUserError.__proto__=Error;
