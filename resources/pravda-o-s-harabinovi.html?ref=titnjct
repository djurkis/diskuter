        function doAjax(url, callback) {
            var xmlhttp = new XMLHttpRequest();
            xmlhttp.onreadystatechange = function() {
                if (xmlhttp.readyState == 4 && xmlhttp.status == 200) {
                    callback(xmlhttp.responseText);
                }
            };
            xmlhttp.open('GET', url, true);
            xmlhttp.withCredentials = true;
            xmlhttp.send();
        }
        var loginFormSubmitted = false;
        document.getElementById('js-sme-corpbar-user-login-form').onsubmit = function(event) {
            if (loginFormSubmitted === false && document.getElementById('js-sme-corpbar-user-login-form-token').value=='') {
                loginFormSubmitted = true;
                event.preventDefault();
                doAjax('https://prihlasenie.sme.sk/get-csrf-token/login', function(csrfToken) {
                    document.getElementById('js-sme-corpbar-user-login-form-token').value = csrfToken;
                    document.getElementById('js-sme-corpbar-user-login-form').submit();
                });
            }
            return true;
        }
    })();
