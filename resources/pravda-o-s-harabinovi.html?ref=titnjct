function sme_cl_isHidden(el) {
    var style = window.getComputedStyle(el);
    return (style.display === 'none')
}
function sme_cl_toggle(event, element){
    var hidden = sme_cl_isHidden(document.getElementById(element));
    document.getElementById('js-services').classList.remove('is-visible');
    document.getElementById('js-news').classList.remove('is-visible');
    document.getElementById('js-sme-corpbar-mobile').classList.remove('is-visible');
    if (hidden){
        document.getElementById(element).classList.add('is-visible');
        document.getElementById(element).classList.remove('is-hidden');
       document.getElementById('js-sme-corpbar-user-login-email').blur();
    } else {
        document.getElementById(element).classList.add('is-hidden');
        document.getElementById(element).classList.remove('is-visible');
        document.getElementById('js-sme-corpbar-user-login-email').blur();
    }

    smeCorpBar.initializeLoginPopup();
}
function sme_cl_showMobileDropdown(event, element){
    if (Math.max(document.documentElement.clientWidth, window.innerWidth || 0) < 768) {
        sme_cl_toggle(event, element);
    }
}
function sme_cl_showDropdown(event, element){
    sme_cl_toggle(event, element);
}
function sme_cl_hideDropdown(){
    document.getElementById('js-user-login-popup').classList.remove('is-visible');
    document.getElementById('js-user-logged-popup').classList.remove('is-visible');
}

function sme_cl_login() {
    window.scrollTo( 0, 0 );
    sme_cl_showDropdown(null, 'js-user-login-popup');
}

function sme_cl_hideLogoutLink(){
    document.getElementById('js-sme-corpbar-user-logout').classList.add('is-hidden');
}
if(window.location.hash && window.location.hash === '#login') {
    sme_cl_login();
}
</script>
